{
	"name": "sgmnt",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"clean": "yarn rimraf artifacts cache typechain-types",
		"build:contracts": "npx hardhat compile",
		"deploy": "hardhat deploy --network",
		"dev": "vite dev --host",
		"build": "vite build && npx hardhat compile",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && check:prettier",
		"check:prettier": "prettier '**/*.{sol,js,ts,tsx,json}' --ignore-path .gitignore --list-different",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"fix:prettier": "prettier '**/*.{sol,js,ts,tsx,json}' --ignore-path .gitignore --write",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"coverage": "hardhat coverage"
	},
	"devDependencies": {
		"@paralleldrive/cuid2": "^2.2.0",
		"@skeletonlabs/skeleton": "^1.1.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-vercel": "^2.4.1",
		"@sveltejs/kit": "^1.5.0",
		"@threlte/core": "^5.0.9",
		"@threlte/extras": "^4.9.3",
		"@threlte/preprocess": "^0.0.2",
		"@types/three": "^0.149.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"dotenv": "^16.0.3",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"lamina": "^1.1.23",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "4.13.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^4.10.7",
		"svelte-sequential-preprocessor": "^0.0.7",
		"tailwindcss": "^3.2.7",
		"three": "^0.150.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vite-plugin-commonjs": "^0.6.2"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "4.13.0",
		"@tanstack/svelte-query": "^4.29.1",
		"@teamhanko/hanko-elements": "0.4.0-alpha",
		"@trpc/server": "10.21.1-alpha.2",
		"hardhat": "^2.14.0",
		"superjson": "^1.12.3",
		"svelte-navigator": "^3.2.2",
		"trpc-svelte-query-adapter": "^2.1.0",
		"trpc-sveltekit": "^3.5.5",
		"zod": "^3.21.4"
	}
}